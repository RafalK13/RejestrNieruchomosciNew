<UserControl x:Class="RejestrNieruchomosciNew.View.UserControl_InfoDanePodst"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RejestrNieruchomosciNew.View"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <StackPanel>
        <TextBox Grid.Column="0"
                  Text="{Binding userControl_prev.infoMain.prev.dzialkaSel.Numer, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                  />

        <TextBox Grid.Column="0"
                  Text="{Binding infoMain.infoPodst.p.dzialkaSel.Numer, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                  />

        <TextBox Grid.Column="0"
                  Text="{Binding infoMain.name, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                  />

        <TextBox Grid.Column="0"
                  Text="{Binding userControl_prev.name1, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                  />

        <TextBox Grid.Column="0"
                  Text="{Binding modeMessage, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                  />
    </StackPanel>
</UserControl>
